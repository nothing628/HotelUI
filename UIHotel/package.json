{
  "name": "asp.net",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node Vue/build/dev-server.js",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o vue/dist/build.css ] -e vue/src/main.js | uglifyjs -c warnings=false -m > vue/dist/build.js",
    "webpack": "webpack",
    "wathcify": "watchify -vd -p browserify-hmr -e vue/src/main.js -t browserify-css --minify=true --output=vue/dist/bundle.css -o vue/dist/build.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      [
        "browserify-css",
        {
          "minify": true,
          "output": "vue/dist/bundle.css"
        }
      ]
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "4.0.0-beta.2",
    "change-case": "^3.0.1",
    "chart.js": "^2.7.1",
    "fullcalendar": "^3.8.0",
    "jquery": "^3.2.1",
    "jquery.flot": "^0.8.3",
    "moment": "^2.20.0",
    "popper.js": "1.12.3",
    "select2": "^4.0.6-rc.1",
    "vue": "^2.5.13",
    "vue-morris": "0.0.13",
    "vuetify": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-css": "^0.14.0",
    "browserify-hmr": "^0.3.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.28.9",
    "envify": "^3.4.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-server": "^0.9.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^2.3.0",
    "proxyquireify": "^3.0.1",
    "sass-loader": "^6.0.6",
    "uglify-js": "^2.5.0",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0",
    "webpack": "^3.11.0"
  }
}
